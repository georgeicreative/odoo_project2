<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="view_web_hobyx2_form" model="ir.ui.view">
        <field name="name">web.hobyx2form</field>
        <field name="model">web.hobyx2</field>
        <field name="arch" type="xml">
            <form>
                <field name="name"/>
                <field name="idh"/>
                <field name="type"/>
            </form>
        </field>
    </record>

    <record id="view_web_hobyx2_tree" model="ir.ui.view">
        <field name="name">web.hobyx2.tree</field>
        <field name="model">web.hobyx2</field>
        <field name="arch" type="xml">
            <form>
                <field name="name"/>
                <field name="idh"/>
                <field name="type"/>
            </form>
        </field>
    </record>

    <record id="action_web_hoby2" model="ir.actions.act_window">
        <field name="name">webhobyx2</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">web.hobyx2</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_hobyx"
              name="hobyx"
              parent="menu_webschool2_hobby"
              action="action_web_hoby2"
              sequence="10"/>
</odoo>